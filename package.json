  "license": "MIT",
  "author": "Ramkumar R",
  "files": [
    "dist"
  ],
  "main": "./dist/draw-sign-pdf.umd.js",
  "module": "./dist/draw-sign-pdf.es.js",
  "exports": {
    ".": {
      "import": "./dist/draw-sign-pdf.es.js",
      "require": "./dist/draw-sign-pdf.umd.js"
    }
  },
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:demo": "vite build --mode demo",
    "preview": "vite preview",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore ./lib ./demo --fix",
    "format": "prettier ./lib ./demo  -w -u"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3"
  },
  "devDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/user-event": "^14.0.0-beta",
    "@testing-library/vue": "^6.4.2",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "highlight.js": "^11.4.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.1.27",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.29.8"
  }
}
